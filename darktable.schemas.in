    <schema>
      <key>/schemas/apps/darktable/database</key>
      <applyto>/apps/darktable/database</applyto>
      <owner>darktable</owner>
      <type>string</type>
      <default>.darktabledb</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>database file</short>
        <long>filename relative to your home directory, or starting with a slash.</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/darktable/database_cache_thumbnails</key>
      <applyto>/apps/darktable/database_cache_thumbnails</applyto>
      <owner>darktable</owner>
      <type>int</type>
      <default>2500</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>thumbnails to keep in database</short>
        <long>how many most-recently used thumbnail mipmaps should be kept in the database cache when darktable closes.</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/darktable/database_cache_thumbnails0</key>
      <applyto>/apps/darktable/database_cache_thumbnails0</applyto>
      <owner>darktable</owner>
      <type>int</type>
      <default>10000</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>very small thumbnails to keep in database</short>
        <long>how many smallest-size most-recently used thumbnail mipmaps should be kept in the database cache when darktable closes. these do not need a lot of memory and provide fast visual feedback for lighttable mode.</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/darktable/mipmap_cache_thumbnails</key>
      <applyto>/apps/darktable/mipmap_cache_thumbnails</applyto>
      <owner>darktable</owner>
      <type>int</type>
      <default>500</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>small thumbnails to keep in main memory cache</short>
        <long>how many smallest-size thumbnails to keep in main memory at maximum. this also affects larger mipmap counts,
              these get fewer by size. only read at startup.</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/darktable/mipmap_cache_full_images</key>
      <applyto>/apps/darktable/mipmap_cache_full_images</applyto>
      <owner>darktable</owner>
      <type>int</type>
      <default>2</default>
      <gettext_domain>darktable</gettext_domain>
      <locale name="C">
        <short>full resolution image buffers to keep in main memory cache</short>
        <long>how many full-resolution floating point buffers of loaded raw images to keep in main memory at maximum. only read at startup.</long>
      </locale>
    </schema>

