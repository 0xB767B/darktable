msgid "0 -- append on top of stack, 1 -- replace it."
msgstr "0 -- oben auf den Stapel hinzufügen, 1 -- ersetze ihn."

#: ../darktable.schemas.in.h:3
msgid "0: mean, 1: min, 2: max"
msgstr "0: Mittel, 1: Minimum, 2: Maximum"

